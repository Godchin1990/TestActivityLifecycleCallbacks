<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.windward.www.casio_golf_viewer.casio.golf.activity.LicenseActivity">

    <RelativeLayout
        android:id="@+id/titleRelativeLayout"
        style="@style/title_style"
        android:background="@color/main_bg"
        >
        <RelativeLayout
            android:id="@+id/backRelativeLayout"
            android:layout_width="200px"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/backImageView"
                android:layout_width="39px"
                android:layout_height="57px"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_back_gr"/>
        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:textSize="50px"
            android:text="@string/information_title"
            android:gravity="center"
            android:textStyle="bold"
            android:layout_centerInParent="true"/>
    </RelativeLayout>

</RelativeLayout>
